{
    "$schema": "https://vega.github.io/schema/vega/v5.json",
    "width": 500,
    "height": 300,
    "data": [
      {
        "name": "world",
        "url": "https://vega.github.io/vega/data/world-110m.json",
        "format": {"type": "topojson", "feature": "countries"}
      }
    ],
    "projections": [
      {
        "name": "projection",
        "type": "mercator"
      }
    ],
    "scales": [
        {
          "name": "zoom",
          "type": "zoom",{
            "$schema": "https://vega.github.io/schema/vega/v5.json",
            "width": 500,
            "height": 300,
            "config": {
              "view": {
                "continuousWidth": 500,
                "continuousHeight": 300
              }
            },
            "data": [
              {
                "name": "world",
                "url": "your_data_url.geojson",
                "format": {"type": "geojson"}
              }
            ],
            "scales": [
              {
                "name": "x",
                "type": "identity",
                "domain": [0, 1]
              },
              {
                "name": "y",
                "type": "identity",
                "domain": [0, 1]
              }
            ],
            "projections": [
              {
                "name": "projection",
                "type": "identity",
                "scale": 500
              }
            ],
            "marks": [
              {
                "type": "geoshape",
                "from": {"data": "world"},
                "encode": {
                  "enter": {
                    "fill": {"value": "lightgray"},
                    "stroke": {"value": "white"},
                    "strokeWidth": {"value": 1}
                  }
                },
                "transform": [
                  {
                    "type": "geoshape",
                    "projection": "projection"
                  }
                ]
              }
            ],
            "config": {
              "view": {"stroke": null}
            },
            "signals": [
              {
                "name": "pan",
                "value": [0, 0],
                "on": [
                  {"events": "@pan", "update": "[x(), y()]"}
                ]
              },
              {
                "name": "wheel",
                "value": 0,
                "on": [
                  {"events": "wheel", "update": "pow(1.001, event.deltaY * -0.01)"}
                ]
              }
            ],
            "marks": [
              {
                "type": "geoshape",
                "from": {"data": "world"},
                "encode": {
                  "enter": {
                    "fill": {"value": "lightgray"},
                    "stroke": {"value": "white"},
                    "strokeWidth": {"value": 1}
                  }
                },
                "transform": [
                  {
                    "type": "geoshape",
                    "projection": "projection"
                  }
                ]
              }
            ],
            "projections": [
              {
                "name": "projection",
                "type": "identity",
                "translate": {"signal": "pan"},
                "scale": {"signal": "wheel * 500"}
              }
            ]
          }
          
          "zoomExtent": [1, 10]
        }
      ],
      
    "marks": [
      {
        "type": "shape",
        "from": {"data": "world"},
        "encode": {
          "enter": {
            "fill": {"value": "steelblue"},
            "fillOpacity": {"value": 0.7}
          }
        },
        "transform": [
          {"type": "geoshape", "projection": "projection"}
        ]
      }
    ],
    "signals": [
      {
        "name": "zoom",
        "value": 1,
        "on": [
          {"events": {"type": "wheel", "consume": true}, "update": "pow(1.001, event.deltaY * pow(16, event.deltaMode))"}
        ]
      }
    ]
  }
  