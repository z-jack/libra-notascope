{
    "$schema": "https://vega.github.io/schema/vega/v5.json",
    "width": 500,
    "height": 300,
    "data": [
      {
        "name": "world",
        "url": "https://vega.github.io/vega/data/world-110m.json",
        "format": {"type": "topojson", "feature": "countries"}
      }
    ],
    "projections": [
      {
        "name": "projection",
        "type": "mercator"
      }
    ],
    "scales": [
        {
          "name": "zoom",
          "type": "linear"
        }
      ],
      
    "marks": [
      {
        "type": "shape",
        "from": {"data": "world"},
        "encode": {
          "enter": {
            "fill": {"value": "steelblue"},
            "fillOpacity": {"value": 0.7}
          }
        },
        "transform": [
          {"type": "geoshape", "projection": "projection"}
        ]
      }
    ],
    "signals": [
      {
        "name": "zoom",
        "value": 1,
        "on": [
          {"events": {"type": "wheel", "consume": true}, "update": "pow(1.001, event.deltaY * pow(16, event.deltaMode))"}
        ]
      }
    ]
  }
  